{
  "name": "eg_manta",
  "title": "Manta SV Caller",
  "summary": "Manta SV caller for single samples",
  "dxapi": "1.0.0",
  "version": "0.0.1",
  "inputSpec": [
    {
      "name": "bams",
      "class": "array:file",
      "optional": false,
      "patterns": [
        "*"
      ],
      "help": "Select paired .bam and .bai for a single sample."
    },
    {
      "name": "family_number",
      "class": "string",
      "optional": false,
      "help": ""
    },
    {
      "name": "sample_number",
      "class": "string",
      "optional": false,
      "help": ""
    }
  ],
  "outputSpec": [
    {
      "name": "results",
      "class": "array:file",
      "patterns": [
        "*"
      ],
      "help": ""
    }
  ],
  "runSpec": {
    "timeoutPolicy": {
      "*": {
        "hours": 1
      }
    },
    "interpreter": "python3",
    "file": "src/eg_manta.py",
    "distribution": "Ubuntu",
    "release": "20.04",
    "version": "0"
  },
  "access": {
    "project": "CONTRIBUTE",
    "allProjects": "CONTRIBUTE",
    "network": [
      "*"
    ]
  },
  "regionalOptions": {
    "aws:eu-central-1": {
      "systemRequirements": {
        "*": {
          "instanceType": "mem1_ssd1_v2_x8"
        }
      }
    }
  }
}
